{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": {
          "node": "10.4"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": 3,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": []
    }
  }
}
